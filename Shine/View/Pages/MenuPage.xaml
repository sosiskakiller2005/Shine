<Page x:Class="Shine.View.Pages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Shine.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="1000"
      Title="MenuPage">

    <StackPanel>
        <Border Height="40"/>
        <Image Source="/Resources/logo.png" Height="100"/>
        <Border Height="100"/>
        <Button Name="CatalogBtn" Content="Каталог" Click="CatalogBtn_Click"/>
        <Border Height="20"/>
        <Button Name="OrdersBtn" Content="Заказы" Click="OrdersBtn_Click"/>
    </StackPanel>
</Page>
